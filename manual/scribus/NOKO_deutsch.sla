            <para ALIGN="1"/>
            <para ALIGN="1"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="30" FCOLOR="NOKOdunkelgrau" CH="Das Dis­play"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT CH="Hier zeigt NO­KO al­so Da­tum und Uhr­zeit an. Aber was be­deu­ten die­se andere Zahl und die­se Buch­sta­ben?"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="La­de­stand"/>
            <para ALIGN="3"/>
            <ITEXT CH="NO­KO braucht Strom. Sein Ak­ku hält et­wa einen Tag, der La­de­stand wird oben rechts als Zahl in Prozent an­ge­zeigt. "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="%"/>
            <tab/>
            <ITEXT CH="Der Ak­ku ent­lädt."/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Bold Italic" FONTSIZE="15" FCOLOR="Noko" CH="+"/>
            <ITEXT CH=" "/>
            <tab/>
            <ITEXT CH="Der Ak­ku wird ge­la­den."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT CH="Fällt der La­de­stand un­ter "/>
            <ITEXT FCOLOR="Noko" CH="10%"/>
            <ITEXT CH=", so blinkt die An­zei­ge und NO­KO be­schwert sich je­de Mi­nu­te mit ei­nem Piep­ton. "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Ein­stel­lun­gen"/>
            <para ALIGN="3"/>
            <ITEXT CH="An­hand ei­nes Buch­sta­bens wer­den ak­tu­el­le Ein­stel­lun­gen dar­ge­stellt. Beim ers­ten Ein­schal­ten soll­te hier ein "/>
            <ITEXT FCOLOR="Noko" CH="n"/>
            <ITEXT CH=" stehen."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="n"/>
            <tab FONT="Frutiger Linotype Bold Italic" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="Nacht­mo­dus ist ein­ge­stellt."/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="N"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="Nacht­mo­dus ist ein­ge­stellt und ak­tiv."/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="P"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="NO­KO spielt etwas vor "/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="p"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="Pau­se, die Wiedergabe wird an­ge­hal­ten."/>
            <para ALIGN="3" INDENT="13.5" FIRST="-13.5">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="X"/>
            <tab/>
            <ITEXT CH="Di­stanz­sen­sor ist de­ak­ti­viert."/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="D"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="Dis­play ist ge­dimmt und geht von al­lei­ne aus."/>
            <para ALIGN="3"/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="Ω"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <tab/>
            <ITEXT CH="(Ome­ga) Der Alarm ist ge­stellt."/>
            <para ALIGN="3" LINESP="15" INDENT="13.5" FIRST="-13.5">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko" CH="A"/>
            <tab FONT="Frutiger Linotype Regular" FONTSIZE="15" FCOLOR="Noko"/>
            <ITEXT CH="Der AUX-Ein­gang ist ak­ti­viert."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT CH="Die ein­zel­nen Ein­stel­lungs­mög­lich­kei­ten wer­den im näch­ten Ab­schnitt vor­ge­stellt."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Was kann man hier noch ma­chen?"/>
            <para ALIGN="3"/>
            <ITEXT CH="Drei Tas­ten kön­nen hier ge­drückt wer­den. Nun­ja, ei­gent­lich vier, aber die Na­sen­tas­te wird nicht ge­braucht und NO­KO mag das nicht. Aus­pro­bie­ren!"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FCOLOR="Noko" CH="Bauchtas­te"/>
            <tab/>
            <tab/>
            <ITEXT CH="Haupt­me­nü"/>
            <para ALIGN="3"/>
            <ITEXT FCOLOR="Noko" CH="Lin­ke Hand"/>
            <tab/>
            <tab/>
            <ITEXT CH="Dis­play dim­men bzw. ausschalten"/>
            <para ALIGN="3"/>
            <ITEXT FCOLOR="Noko" CH="Rech­te Hand"/>
            <tab/>
            <tab/>
            <ITEXT CH="Di­stanz­sen­sor an/aus"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Na­nu? Was pas­siert da auf dem Dis­play?"/>
            <para ALIGN="3"/>
            <ITEXT CH="Kei­ne Uhr­zeit zu se­hen? Da­für aber ein Ge­dicht? Oder ein dum­mer Spruch? Oder ko­mi­sche Din­ge? Das sind "/>
            <ITEXT FCOLOR="Noko" CH="Events"/>
            <ITEXT CH=", mit de­nen NO­KO sich die Zeit ver­treibt. Da­zu spä­ter."/>
            <trail ALIGN="3"/>
