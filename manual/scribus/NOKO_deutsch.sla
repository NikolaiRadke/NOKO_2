            <ITEXT CH=" ge­setzt, so wird das Dis­play "/>
            <tab/>
            <ITEXT CH="während der an­ge­wähl­ten Zeit au­to­ma­tisch "/>
            <tab/>
            <ITEXT CH="nach "/>
            <ITEXT FCOLOR="Noko" CH="ei­ner Mi­nu­te"/>
            <ITEXT CH="  aus­ge­schal­tet."/>
            <para ALIGN="3" INDENT="13.5" FIRST="-13.5">
