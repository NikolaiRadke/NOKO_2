            <ITEXT CH=" an­ge­wählt wer­den, an der ent­spre­chen­den Stel­le er­scheint ein "/>
            <ITEXT FCOLOR="Noko" CH="X"/>
            <ITEXT CH="."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FCOLOR="Noko" CH="Ak­ku spa­ren"/>
            <tab FCOLOR="Noko"/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
            <ITEXT CH="Ist NO­KO nicht am La­de­ge­rät und wird das Dis­play au­to­ma­tisch oder ma­nu­ell ge­dimmt, so wird ein stärkerer Strom­spar­mo­dus ak­tiv, bei dem u.a. das Dis­play ganz aus­ge­schal­tet wird."/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
            <ITEXT FCOLOR="NOKOdunkelgrau" CH="Hin­weis:"/>
            <ITEXT CH=" Die "/>
            <ITEXT FCOLOR="Noko" CH="Strom­spar­me­cha­nis­men"/>
            <ITEXT CH=" wer­den im nächs­ten Ka­pi­tel be­han­delt."/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
            <para ALIGN="3"/>
            <ITEXT FCOLOR="Noko" CH="Di­stanz­licht"/>
            <tab FCOLOR="Noko"/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
            <ITEXT CH="Ist das Dis­play ge­dimmt, so kann es mit ei­nem "/>
            <ITEXT FCOLOR="Noko" CH="Wink"/>
            <ITEXT CH=" vor NO­KOs Di­stanz­sen­sor wie­der be­leuch­tet wer­den, um z.B. nachts die Uhr­zeit ab­le­sen zu kön­nen. An­schlie­ßend wird es nach kur­z­er Zeit wie­der ge­dimmt. Ist der Di­stanz­sen­sor auf "/>
            <ITEXT FCOLOR="Noko" CH="0"/>
            <ITEXT CH=" ge­stellt oder durch Drücken der "/>
            <ITEXT FCOLOR="Noko" CH="rech­ten Hand"/>
            <ITEXT CH=" de­ak­ti­viert, so steht in der Uhr­an­zei­ge ein "/>
            <ITEXT FCOLOR="Noko" CH="X"/>
            <ITEXT CH=" und das Di­stanz­licht funk­tio­niert nicht."/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="95.75" Fill=""/>
            </para>
