            <para ALIGN="1"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Spiel was vor"/>
            <para ALIGN="3"/>
            <ITEXT CH="Ist ei­nes der Ge­rä­te ge­ra­de ak­tiv, wird in die­sem Me­nü vor dem je­wei­li­gen Ein­trag ein "/>
            <ITEXT FCOLOR="Noko" CH="No­ten­sym­bol "/>
            <ITEXT FONT="Frutiger Linotype Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="♪"/>
            <ITEXT CH=" an­ge­zeigt."/>
            <para ALIGN="3" INDENT="0" FIRST="0">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <para ALIGN="3" INDENT="0" FIRST="0">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <ITEXT FONTSIZE="12" FCOLOR="Noko" CH="Ra­dio hö­ren"/>
            <tab FONTSIZE="12" FCOLOR="Noko"/>
            <para ALIGN="3"/>
            <ITEXT CH="Hier können das Ra­dio ein­ge­schal­tet und ver­schie­de­ne Sen­der auf Sta­ti­ons­tas­ten ver­teilt wer­den."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="12" FCOLOR="Noko" CH="* Eigenes hö­ren"/>
            <tab FONTSIZE="12" FCOLOR="Noko"/>
            <para ALIGN="3"/>
            <ITEXT CH="NO­KO eigene MP3s wiedergeben. Hier in die­sem Un­ter­me­nü. Sofern der Erbauer welche aufgespielt hat."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="12" FCOLOR="Noko" CH="AUX-Ein­gang"/>
            <para ALIGN="3"/>
            <ITEXT CH="Zum Hören eines externen Gerätes ge­nügt ein Druck auf die "/>
            <ITEXT FCOLOR="Noko" CH="Bauchtas­te"/>
            <ITEXT CH=", wor­auf­hin ein "/>
            <ITEXT FCOLOR="Noko" CH="No­ten­sym­bol"/>
            <ITEXT CH=" vor dem Menü­punkt er­scheint. Soll­ten das Ra­dio oder ei­ne Ge­schich­te lau­fen, so wer­den die­se al­ler­dings aus­ge­schal­tet."/>
            <para ALIGN="3" INDENT="0" FIRST="0">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="12" FCOLOR="Noko" CH="* Ge­schich­ten hö­ren"/>
            <tab FONTSIZE="12" FCOLOR="Noko"/>
            <para ALIGN="3"/>
            <ITEXT CH="NO­KO kann Hör­spie­le er­zäh­len. Al­les hier in die­sem Un­ter­me­nü. Sofern der Erbauer auch hier  welche aufgespielt hat."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Ra­dio hö­ren"/>
            <para ALIGN="3"/>
            <ITEXT CH="Das Ra­dio. Die nie ver­sie­gen­de Quel­le der Un­ter­hal­tung. Die ers­te Zei­le zeigt die "/>
            <ITEXT FCOLOR="Noko" CH="Fre­quenz"/>
            <ITEXT CH=" in MHz und, so­fern der Emp­fang gut ge­nug ist, den "/>
            <ITEXT FCOLOR="Noko" CH="Sen­der­na­men"/>
            <ITEXT CH=" via RDS an. Das Ra­dio ist, wenn es nicht in die­sem Me­nü zu­vor an­ge­schal­tet wur­de, aus. Ist das Ra­dio an und wird das Me­nü an­hand der "/>
            <ITEXT FCOLOR="Noko" CH="Na­sen­tas­te"/>
            <ITEXT CH=" ver­las­sen, läuft es wei­ter, bis es aus­ge­schal­tet wird."/>
            <para ALIGN="3"/>
            <para ALIGN="3" INDENT="13.5" FIRST="-13.5">
                <Tabs Type="0" Pos="13" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
            </para>
            <ITEXT FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="[«][»]"/>
            <tab/>
            <ITEXT CH="Sen­der­such­lauf. NO­KO ver­sucht, einen Sen­der "/>
            <tab/>
            <ITEXT CH="zu fin­den. Da­zu muss das Ra­dio je­doch "/>
            <tab/>
            <ITEXT CH="eingeschaltet sein."/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="68.75" Fill=""/>
            </para>
            <ITEXT FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="[&lt;][&gt;]"/>
            <tab/>
            <ITEXT CH="Ma­nu­el­le Su­che. Än­dert die Fre­quenz um "/>
            <ITEXT FCOLOR="Noko" CH="0.1"/>
            <ITEXT CH=" "/>
            <tab/>
            <ITEXT CH="MHz."/>
            <para ALIGN="3">
                <Tabs Type="0" Pos="68.75" Fill=""/>
            </para>
            <ITEXT FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="[►][■]"/>
            <tab FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2"/>
            <ITEXT CH="Star­tet/Stoppt das Ra­dio. Läuft gerade ei­ne "/>
            <tab/>
            <ITEXT CH="Geschich­te oder ist der AUX-Ein­gang ak­tiv, so "/>
            <tab/>
            <ITEXT CH="wer­den die­se ab­ge­schal­tet."/>
            <para ALIGN="3" INDENT="13.5" FIRST="-13.5">
                <Tabs Type="0" Pos="68.75" Fill=""/>
                <Tabs Type="0" Pos="68.75" Fill=""/>
                <Tabs Type="0" Pos="68.75" Fill=""/>
                <Tabs Type="0" Pos="68.75" Fill=""/>
