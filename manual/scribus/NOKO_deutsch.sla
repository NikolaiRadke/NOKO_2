            <ITEXT FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="[R]"/>
            <tab/>
            <ITEXT CH="Star­tet RDS-Text in "/>
            <ITEXT FCOLOR="Noko" CH="Zei­le zwei"/>
            <ITEXT CH=", so­fern das "/>
            <tab/>
            <ITEXT CH="Radio eingeschaltet ist. Bei schlechtem "/>
            <tab/>
            <ITEXT CH="Empfang ist "/>
            <tab/>
            <ITEXT CH="hier lei­der nichts zu se­hen. Oder "/>
