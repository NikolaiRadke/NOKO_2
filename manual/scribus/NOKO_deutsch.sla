            <ITEXT FONT="FreeSans Regular" FONTSIZE="30" FCOLOR="NOKOdunkelgrau" CH="Fragen und Anworten"/>
            <para ALIGN="0" INDENT="13.5" FIRST="-13.5">
                <Tabs Type="0" Pos="12.5" Fill=""/>
                <Tabs Type="0" Pos="13" Fill=""/>
