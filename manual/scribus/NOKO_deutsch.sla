            <ITEXT FONTSIZE="16" FCOLOR="NOKOdunkelgrau" CH="Nacht­mo­dus"/>
            <para ALIGN="3"/>
            <ITEXT CH="Manch­mal ist NO­KO ganz schön red­se­lig. Zum Glück gibt es den Nacht­mo­dus! In ei­nem an­ge­ge­be­nen Zeit­raum, vor­ein­ge­stellt ist 22:00 bis 09:00 Uhr, hält NO­KO sich vor­nehm zu­rück. In die­sem Zeit­raum"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <tab FONT="Frutiger Linotype Regular" FCOLOR="Noko"/>
            <ITEXT FONT="Frutiger Linotype Regular" FCOLOR="Noko" CH="•"/>
            <tab FONT="Frutiger Linotype Regular"/>
            <ITEXT FONT="Frutiger Linotype Regular" CH="löst der Di­stanz­sen­sor kei­ne Spra­che aus"/>
            <ITEXT CH="."/>
            <para ALIGN="3"/>
            <tab FONT="Frutiger Linotype Regular" FCOLOR="Noko"/>
            <ITEXT FONT="Frutiger Linotype Regular" FCOLOR="Noko" CH="•"/>
            <tab FONT="Frutiger Linotype Regular"/>
            <ITEXT FONT="Frutiger Linotype Regular" CH="sagt NO­KO nichts"/>
            <ITEXT CH=", außer man drückt Tasten."/>
            <para ALIGN="3"/>
            <tab FONT="Frutiger Linotype Regular" FCOLOR="Noko"/>
            <ITEXT FONT="Frutiger Linotype Regular" FCOLOR="Noko" CH="•"/>
            <tab FONT="Frutiger Linotype Regular" FCOLOR="Noko"/>
            <ITEXT CH="wird ein Strom­spar­mo­dus ak­tiv."/>
            <para ALIGN="3"/>
            <tab/>
            <ITEXT FONT="Frutiger Linotype Regular" FCOLOR="Noko" CH="•"/>
            <tab FONT="Frutiger Linotype Regular" FCOLOR="Noko"/>
            <ITEXT CH="kann das Dis­play au­to­ma­tisch dim­men."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="Courier New Regular" FONTSIZE="12" FCOLOR="Noko" SCALEH="119" KERN="2" CH="[an]"/>
            <tab/>
            <ITEXT CH="Hier wird der Nachtmodus ein- bzw. "/>
            <tab/>
            <ITEXT CH="ausgeschaltet. Ist der Nachtmodus an, so "/>
            <tab/>
            <ITEXT CH="erscheint in der Uhzeitanzeige ein "/>
            <ITEXT FCOLOR="Noko" CH="n"/>
            <ITEXT CH="."/>
            <para ALIGN="3" INDENT="13.5" FIRST="-13.5">
